pipeline {
   agent{ label "jenkins_agent"}
    stages {
        stage('AT') {
            steps {
              script{
              try{
              build 'freestyle_demo'
              }catch(e){
              echo "build is failed"
              }  
            }
        }
        }
          post {
        success {
            echo 'Build successful!'
        }
        failure {
            echo 'Build failed!'
        }
    }
    
    stage('Cleanup') {
        steps {
            node {
                // Add cleanup steps here
                echo 'Cleaning up workspace...'
                deleteDir()
            }
        }
    }
    }
}
        
    
   